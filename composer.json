{
  "name": "a7-tech/laminas-attribute-controller",
  "description": "Package for Laminas Framework to use attributes sugar in controller.",
  "type": "library",
  "license": "MIT",
  "repositories": [],
  "require": {
    "php": "^8.2",
    "laminas/laminas-mvc": "^3.0",
    "beberlei/assert": "^3.3",
    "doctrine/orm": "^2.0 | ^3.0",
    "jms/serializer": "^3.30",
    "symfony/validator": "^5.4 | ^6.0 | ^7.0"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.12",
    "phpunit/phpunit": "^9.0",
    "friendsofphp/php-cs-fixer": "^3.75"
  },

  "scripts": {
    "cs": "./vendor/bin/php-cs-fixer fix src",
    "stan": "./vendor/bin/phpstan"
  },
  "autoload": {
    "psr-4": {
      "LaminasAttributeController\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  }
}
